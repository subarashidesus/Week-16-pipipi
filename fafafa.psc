Proceso sin_titulo
	Definir i Como Entero;
	Definir verdura Como Caracter;
	Dimension verdura[4];
	
	
	Para i <- 1 hasta 3 Hacer
		Escribir "Ingrese una verdura en la casilla [ ", i , " ] ";
		Leer verdura[i];
	FinPara
	
	Para i <- 1 hasta 3 Hacer
		Escribir sin saltar verdura[i] " - ";
	FinPara
	
	
FinProceso
